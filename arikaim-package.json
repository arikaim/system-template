{    
    "title": "Control Panel",
    "name": "system",
    "package-type": "template",
    "description": "Arikaim CMS Control Panel",
    "repository": "https://github.com/arikaim/system-template.git",
    "version": "1.11.10",
    "icon": "sliders horizontals",
    "image": {
        "src": "",
        "alt": ""
    },
    "languages": [
        "en"       
    ],
    "themes": [
        "default"
    ], 
    "require": {
        "library": [
            "arikaim",
            "arikaim-ui",
            "jquery",
            "highlight",
            "jquery-ui",
            "fomantic",
            "tailwind",
            "filepond"
        ],
        "extensions": [
            "dashboard"
        ]
    },
    "include": {            
        "css": [
            "style.css",
            "tailwind.min.css"
        ],
        "js": [
            "control-panel.js"
        ],
        "components": [
            "system:users",
            "semantic~language.translate",
            "semantic~modal"    
        ],
        "library": [
            "jquery",
            "arikaim:admin",
            "arikaim-ui",
            "arikaim-ui:form",
            "fomantic",
            "highlight:github",
            "filepond",
            "markdown-editor"      
        ]
    }
}